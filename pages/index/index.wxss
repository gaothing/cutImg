page{
  background: rgba(0, 0, 0, 1)
}
.box {
  width: 200px;
  height: 200px;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  border: 1px solid #ffffff;
  z-index: 1;
}

.box2 {
  left: 100%;
}

.zhe {
  position: fixed;
  z-index: 2222;
  height: 100%;
  background: rgba(0, 0, 0, 0.2);
}

.zheleft {
  left: 0;
  top: 0;
}

.zheright {
  top: 0;
  right: 0;
}

.zhetop {
  top: 0;
  width: 200px;
}

.zhebottom {
  width: 200px;
  bottom: 0;
}

canvas {
  position: fixed;
  left: 100%;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #eee;
  margin-top: 50px;
  opacity: 0;
}



.btn {
  position: fixed;
  right: 60rpx;
  bottom: 60rpx;
  width: 200rpx;
  margin-top: 20rpx;
  background: #327ff1;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  border-radius: 20rpx;
  z-index: 999999
}
.btn1 {
  position: fixed;
  left: 60rpx;
  bottom: 60rpx;
  width: 200rpx;
  margin-top: 20rpx;
  background: #327ff1;
  height: 50px;
  line-height: 50px;
  text-align: center;
  color: #fff;
  font-size: 20px;
  border-radius: 20rpx;
  z-index: 999999
}
.imgBox{
  position: absolute;
  left: 0;
  top: 0;
  width: 400rpx;
  height: auto
}
.imgBox image{
  width: 100%
}